<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.mailclient.SaveUnfinishedMsg"
            prefHeight="400.0" prefWidth="600.0" alignment="CENTER">
    <Label>Save unfinished message and finish it later?</Label>
    <HBox alignment="CENTER">
        <Button fx:id="btnCancel" onAction="#onClickBtnCancel" cancelButton="true">No</Button>
        <Button fx:id="btnSaveMsg" onAction="#onClickBtnSaveMsg" defaultButton="true">Yes</Button>
    </HBox>
</VBox>
